[run]
omit = everest/compat.py
       everest/ini.py
       everest/flexfilter.py
       everest/rfc3339.py
       everest/testing.py
       everest/tests/*
       everest/repositories/rdb/testing.py
       everest/repositories/nosql/testing.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError

[html]
extra_css = .cov-style.css
